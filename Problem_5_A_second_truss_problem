      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# ============================================================\n",
        "# 2D Truss FEM (Problem 5) - MATLAB -> Python (Google Colab)\n",
        "# FULL CODE (copy & run in 1 cell)\n",
        "# ============================================================\n",
        "\n",
        "def formStiffness2Dtruss(GDof, numberElements, elementNodes, nodeCoordinates, xx, yy, EA):\n",
        "    K = np.zeros((GDof, GDof), dtype=float)\n",
        "\n",
        "    for e in range(numberElements):\n",
        "        n1, n2 = elementNodes[e, 0], elementNodes[e, 1]  # 1-based\n",
        "\n",
        "        # MATLAB edof = [n1*2-1 n1*2 n2*2-1 n2*2]\n",
        "        edof = np.array([n1*2-1, n1*2, n2*2-1, n2*2], dtype=int) - 1  # 0-based\n",
        "\n",
        "        xa = xx[n2-1] - xx[n1-1]\n",
        "        ya = yy[n2-1] - yy[n1-1]\n",
        "        L = np.sqrt(xa*xa + ya*ya)\n",
        "\n",
        "        C = xa / L\n",
        "        S = ya / L\n",
        "\n",
        "        k_local = (EA / L) * np.array([\n",
        "            [ C*C,  C*S, -C*C, -C*S],\n",
        "            [ C*S,  S*S, -C*S, -S*S],\n",
        "            [-C*C, -C*S,  C*C,  C*S],\n",
        "            [-C*S, -S*S,  C*S,  S*S],\n",
        "        ], dtype=float)\n",
        "\n",
        "        K[np.ix_(edof, edof)] += k_local\n",
        "\n",
        "    return K\n",
        "\n",
        "\n",
        "def solution(GDof, prescribedDof, K, F, u_prescribed=None):\n",
        "    if u_prescribed is None:\n",
        "        u_prescribed = np.zeros(len(prescribedDof), dtype=float)\n",
        "\n",
        "    prescribed = np.array([d-1 for d in prescribedDof], dtype=int)  # 0-based\n",
        "    all_dofs = np.arange(GDof, dtype=int)\n",
        "    free = np.setdiff1d(all_dofs, prescribed)\n",
        "\n",
        "    U = np.zeros(GDof, dtype=float)\n",
        "    U[prescribed] = u_prescribed\n",
        "\n",
        "    K_ff = K[np.ix_(free, free)]\n",
        "    K_fp = K[np.ix_(free, prescribed)]\n",
        "    F_f = F[free]\n",
        "\n",
        "    rhs = F_f - K_fp @ U[prescribed]\n",
        "    U[free] = np.linalg.solve(K_ff, rhs)\n",
        "\n",
        "    return U\n",
        "\n",
        "\n",
        "def drawingMesh(nodeCoordinates, elementNodes, style='k--', label=None):\n",
        "    for (n1, n2) in elementNodes:\n",
        "        x = [nodeCoordinates[n1-1, 0], nodeCoordinates[n2-1, 0]]\n",
        "        y = [nodeCoordinates[n1-1, 1], nodeCoordinates[n2-1, 1]]\n",
        "        plt.plot(x, y, style, linewidth=2, label=label)\n",
        "        label = None\n",
        "\n",
        "\n",
        "def outputDisplacementsReactions(U, K, GDof, prescribedDof, F):\n",
        "    R = K @ U - F\n",
        "\n",
        "    print(\"=== DISPLACEMENTS (U) ===\")\n",
        "    for i in range(GDof):\n",
        "        print(f\"DOF {i+1}: U = {U[i]:.6e}\")\n",
        "\n",
        "    print(\"\\n=== REACTIONS AT PRESCRIBED DOFs ===\")\n",
        "    for d in prescribedDof:\n",
        "        print(f\"DOF {d}: R = {R[d-1]:.6e}\")\n",
        "\n",
        "\n",
        "def stresses2Dtruss(numberElements, elementNodes, xx, yy, U, E):\n",
        "    sigma = np.zeros(numberElements, dtype=float)\n",
        "\n",
        "    for e in range(numberElements):\n",
        "        n1, n2 = elementNodes[e, 0], elementNodes[e, 1]\n",
        "        edof = np.array([n1*2-1, n1*2, n2*2-1, n2*2], dtype=int) - 1  # 0-based\n",
        "\n",
        "        xa = xx[n2-1] - xx[n1-1]\n",
        "        ya = yy[n2-1] - yy[n1-1]\n",
        "        L = np.sqrt(xa*xa + ya*ya)\n",
        "\n",
        "        C = xa / L\n",
        "        S = ya / L\n",
        "\n",
        "        u_e = U[edof]\n",
        "        sigma[e] = (E / L) * np.array([-C, -S, C, S], dtype=float) @ u_e\n",
        "\n",
        "    print(\"\\nstresses\")\n",
        "    print(sigma)\n",
        "    return sigma\n",
        "\n",
        "\n",
        "# ============================================================\n",
        "# Main (equivalent to problem5.m)\n",
        "# ============================================================\n",
        "\n",
        "# Material\n",
        "E = 70000.0\n",
        "A = 300.0\n",
        "EA = E * A\n",
        "\n",
        "# Connectivity (11 elements)\n",
        "elementNodes = np.array([\n",
        "    [1, 2],\n",
        "    [1, 3],\n",
        "    [2, 3],\n",
        "    [2, 4],\n",
        "    [1, 4],\n",
        "    [3, 4],\n",
        "    [3, 6],\n",
        "    [4, 5],\n",
        "    [4, 6],\n",
        "    [3, 5],\n",
        "    [5, 6]\n",
        "], dtype=int)\n",
        "\n",
        "# Node coordinates (6 nodes)  [x y]\n",
        "nodeCoordinates = np.array([\n",
        "    [0.0,    0.0],\n",
        "    [0.0, 3000.0],\n",
        "    [3000.0, 0.0],\n",
        "    [3000.0, 3000.0],\n",
        "    [6000.0, 0.0],\n",
        "    [6000.0, 3000.0]\n",
        "], dtype=float)\n",
        "\n",
        "numberElements = elementNodes.shape[0]\n",
        "numberNodes = nodeCoordinates.shape[0]\n",
        "\n",
        "xx = nodeCoordinates[:, 0]\n",
        "yy = nodeCoordinates[:, 1]\n",
        "\n",
        "# DOFs\n",
        "GDof = 2 * numberNodes\n",
        "U = np.zeros(GDof, dtype=float)\n",
        "F = np.zeros(GDof, dtype=float)\n",
        "\n",
        "# Loads (MATLAB):\n",
        "# force(4)=-50000; force(8)=-100000; force(12)=-50000;\n",
        "F[3]  = -50000.0   # DOF 4  -> 0-based index 3 (uy node 2)\n",
        "F[7]  = -100000.0  # DOF 8  -> 0-based index 7 (uy node 4)\n",
        "F[11] = -50000.0   # DOF 12 -> 0-based index 11 (uy node 6)\n",
        "\n",
        "# Global stiffness\n",
        "K = formStiffness2Dtruss(GDof, numberElements, elementNodes, nodeCoordinates, xx, yy, EA)\n",
        "\n",
        "# Boundary conditions (MATLAB: prescribedDof=[1 2 10]')\n",
        "# DOF1 ux node1, DOF2 uy node1, DOF10 uy node5\n",
        "prescribedDof = [1, 2, 10]\n",
        "\n",
        "# Solve\n",
        "U = solution(GDof, prescribedDof, K, F)\n",
        "\n",
        "# Plot deformed vs undeformed (MATLAB: scaleFact = 2*dispNorm)\n",
        "us = np.arange(0, GDof, 2)  # ux\n",
        "vs = np.arange(1, GDof, 2)  # uy\n",
        "XX = U[us]\n",
        "YY = U[vs]\n",
        "\n",
        "dispNorm = np.max(np.sqrt(XX**2 + YY**2))\n",
        "scaleFact = 2.0 * dispNorm\n",
        "if dispNorm == 0:\n",
        "    scaleFact = 1.0\n",
        "\n",
        "deformed = nodeCoordinates + scaleFact * np.column_stack([XX, YY])\n",
        "\n",
        "plt.figure(figsize=(9, 4.5))\n",
        "plt.clf()\n",
        "plt.axis('equal')\n",
        "plt.grid(True)\n",
        "plt.title(\"Problem 5 Truss: undeformed vs deformed (scaled)\")\n",
        "\n",
        "drawingMesh(deformed, elementNodes, style='k--', label='Deformed (scaled)')\n",
        "drawingMesh(nodeCoordinates, elementNodes, style='k-', label='Undeformed')\n",
        "\n",
        "plt.legend()\n",
        "plt.show()\n",
        "\n",
        "# Output\n",
        "outputDisplacementsReactions(U, K, GDof, prescribedDof, F)\n",
        "\n",
        "# Element stresses\n",
        "sigma = stresses2Dtruss(numberElements, elementNodes, xx, yy, U, E)\n"
      ]
    }
  ]
}
